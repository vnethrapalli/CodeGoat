datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Feedback {
  id                  Int     @id @default(autoincrement())
  submissionPage      Int
  outputPage          Int
  translationAccuracy Int
  gptAvailability     Int
  experience          Int
  comments            String
}

model FAQ {
  id                  Int     @id @default(autoincrement())
  question            String
  answer              String
}